! Xft
Xft.dpi: 96
Xft.antialias: true
Xft.lcdfilter: lcddefault
Xft.rgba: rgb
Xft.hinting: false
Xft.autohint: true
Xft.hintstyle: hintslight

! Colors
*background: #14181f
*foreground: #b7bdcc

*color0: #2b303b
*color1: #994e55
*color2: #849971
*color3: #c5aa75
*color4: #707e8c
*color5: #8f7189
*color6: #778f8e
*color7: #a6aab2

*color8: #4d576b
*color9: #bf616a
*color10: #a3be8c
*color11: #ebcb8b
*color12: #8fa1b3
*color13: #b48ead
*color14: #96b5b4
*color15: #b9bbbe

*cursorColor: #b7bdcc

! URxvt
URxvt.perl-lib:	/usr/lib/urxvt/perl
URxvt.perl-ext-common:	default,keyboard-select,url-select
URxvt.url-select.launcher: /usr/bin/firefox-bin
URxvt.url-select.underline: true
URxvt.keysym.M-w: perl:url-select:select_next
URxvt.keysym.M-v: perl:keyboard-select:activate

URxvt.iso14755: false
URxvt.iso14755_52: false

URxvt.scrollBar: false
URxvt.internalBorder: 8
!URxvt*cursorUnderline: true
!URxvt.cursorBlink: true

!no bold font
URxvt*italicFont:
URxvt*boldFont:
URxvt*font: -*-tewi-medium-*-*-*-11-*-*-*-*-*-iso10646-*

!vim ft=xdefaults ts=8
